<div class="kt-subheader__main">
    <div class="kt-subheader__breadcrumbs">
        <span class="kt-subheader__breadcrumbs-home"><i class="flaticon2-shelter"></i></span>
        <span class="kt-subheader__breadcrumbs-separator"></span>
        <span class="kt-subheader__breadcrumbs-link kt-subheader__breadcrumbs-link--active">{$assigned.titreModule}</span>
        {if isset($assigned.titreElement) && ($assigned.titreElement neq "")}
            <span class="kt-subheader__breadcrumbs-separator"></span>
            <span class="kt-subheader__breadcrumbs-link kt-subheader__breadcrumbs-link--active">{$assigned.titreElement}</span>
        {/if}
    </div>
    <span class="kt-subheader__separator kt-subheader__separator--v"></span>
    <span class="kt-subheader__desc">{$assigned.titreAction}</span>
</div>